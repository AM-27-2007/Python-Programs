      "source": [
        "# Python program to find the reverse of a given number"
      ],
      "metadata": {
        "id": "ZhGxL6YdH3Yw"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "n = int(input(\"Enter any number : \"))\n",
        "\n",
        "# Defining function\n",
        "def rev(n):\n",
        "  reverse=0\n",
        "  while n!=0:\n",
        "    reverse=reverse*10+n%10\n",
        "    n=n//10\n",
        "  return(reverse)\n",
        "\n",
        "r = rev(n)\n",
        "print(\"The revrse of number %d is %d\"%(n,r))"
      ],
